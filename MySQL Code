

create table election_record(
party_id INT PRIMARY KEY NOT NULL,
PARTY_NAME VARCHAR(255) NOT NULL,
VOTE INT NOT NULL,
VOTE_PERCENTAGE FLOAT NOT NULL);

DESCRIBE ELECTION_RECORD;

SELECT * FROM ELECTION_RECORD;

