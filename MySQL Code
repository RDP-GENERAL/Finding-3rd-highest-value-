select * from election_record
where vote=(select max(vote) from election_record
where vote not in((select max(VOTE) from election_record
where vote not in (select max(VOTE) from election_record)),
(select max(vote) from election_record)));

select* from election_record
order by vote desc
limit 1 offset 2;
